          <xsd:appinfo source="appian.jpa">@Column(name="DateEnded")</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Deadline" nillable="true" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:appinfo source="appian.jpa">@Column(name="Deadline")</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MissedSlaReasonID" nillable="true" type="xsd:int">
        <xsd:annotation>
          <xsd:appinfo source="appian.jpa">@Column(name="MissedSlaReasonID")</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Comment" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:appinfo source="appian.jpa">@Column(name="Comment")</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IsCompleted" nillable="true" type="xsd:int">
        <xsd:annotation>
          <xsd:appinfo source="appian.jpa">@Column(name="isCompleted")</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
