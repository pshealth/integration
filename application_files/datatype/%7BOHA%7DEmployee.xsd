        <xsd:annotation>
          <xsd:appinfo source="appian.jpa">@Column(name="PayRollNo", columnDefinition="nvarchar(255) NULL")</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ShiftWorker" nillable="true" type="xsd:boolean">
        <xsd:annotation>
          <xsd:appinfo source="appian.jpa">@Column(name="ShiftWorker", columnDefinition="bit")</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="WorkingHourCodeId" nillable="true" type="xsd:int">
        <xsd:annotation>
