        <xsd:annotation>
          <xsd:appinfo source="appian.jpa"> @Column(name="IsAdministrator", columnDefinition="BIT NULL")</xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IsHRManager" nillable="true" type="xsd:boolean">
        <xsd:annotation>
          <xsd:appinfo source="appian.jpa"> @Column(name="IsHRManager", columnDefinition="BIT NULL")</xsd:appinfo>
        </xsd:annotation>
