<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>Utils_ConvertBoolean</name>
        <uuid>_a-0000dac7-a419-8000-6ee0-010000010000_4668</uuid>
        <description></description>
        <parentUuid>_a-0000da6a-36ed-8000-6ee0-010000010000_946</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
a: tointeger(input),
if(or(isnull(input), input = &quot;&quot;), &quot;&quot;,
if(a = 1, &quot;Yes&quot;, if(a = 0, &quot;No&quot;, input))))</definition>
        <namedTypedValue>
            <name>input</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
