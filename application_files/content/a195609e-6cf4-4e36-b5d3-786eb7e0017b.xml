<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>Utils_CheckPostCode</name>
        <uuid>a195609e-6cf4-4e36-b5d3-786eb7e0017b</uuid>
        <description>Validate the postcode</description>
        <parentUuid>_a-0000da6a-36ed-8000-6ee0-010000010000_946</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=regexmatch(&quot;(GIR 0AA)|((([A-Z-[QVX]][0-9][0-9]?)|(([A-Z-[QVX]][A-Z-[IJZ]][0-9][0-9]?)|(([A-Z-[QVX]][0-9][A-HJKPSTUW])|([A-Z-[QVX]][A-Z-[IJZ]][0-9][ABEHMNPRVWXY])))) [0-9][A-Z-[CIKMOV]]{2})&quot;,ri!emailValue)</definition>
        <namedTypedValue>
            <name>emailValue</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
