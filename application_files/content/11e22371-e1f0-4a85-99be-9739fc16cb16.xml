    <rule>
        <name>CM_MI_GetLSLAMetIconValues</name>
        <uuid>11e22371-e1f0-4a85-99be-9739fc16cb16</uuid>
        <description>Returns icon values for SLA Met</description>
        <parentUuid>_a-0000db0d-1b74-8000-6ee0-010000010000_10791</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(ri!SLAMet=1,&quot;STATUS_OK&quot;,&#xD;
if(ri!SLAMet=0,if(not(#&quot;_a-0000d33c-c5a1-8000-7bd7-760000760000_1541&quot;(ri!EndDate)),&quot;STATUS_NOTDONE&quot;,if(ri!DeadLine&gt;(now()-7),&quot;RATING_GOOD&quot;,&quot;STATUS_WARN&quot;)),&quot;&quot;))</definition>
        <namedTypedValue>
            <name>SLAMet</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>DeadLine</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>EndDate</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
