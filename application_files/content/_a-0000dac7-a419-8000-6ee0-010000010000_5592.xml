<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>Utils_CalculateAge</name>
        <uuid>_a-0000dac7-a419-8000-6ee0-010000010000_5592</uuid>
        <description></description>
        <parentUuid>_a-0000da6a-36ed-8000-6ee0-010000010000_946</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=if(not(isnull(ri!dob)),
tointeger(split((today()-ri!dob)/365.25,&quot;::&quot;)[1]),
tointeger(null()))</definition>
        <namedTypedValue>
            <name>dob</name>
            <type>
                <name>date</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
