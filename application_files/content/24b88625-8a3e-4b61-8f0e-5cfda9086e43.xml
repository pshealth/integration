<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>AC_CustomerMultiSelect</name>
        <uuid>24b88625-8a3e-4b61-8f0e-5cfda9086e43</uuid>
        <description>Provide Customer facets for multi select facility

</description>
        <parentUuid>_a-0000db81-7a28-8000-5130-010000010000_3511</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>load(&#xD;
  local!FieldsValues,&#xD;
  local!FieldsArray: tostring(&#xD;
    null&#xD;
  ),&#xD;
  local!ValuesArray: tointeger(&#xD;
    null&#xD;
  ),&#xD;
  local!CustomerId,&#xD;
  if(&#xD;
    ri!IsMultiSelect,&#xD;
    with(&#xD;
      local!FieldsValues: #&quot;b5d04606-bdcd-4e24-ab5e-cb084863b446&quot;(&#xD;
        ri!QueryCondition&#xD;
      ),&#xD;
      local!FieldsArray: #&quot;e34303b0-842a-4916-84b4-c22746471617&quot;(&#xD;
        local!FieldsValues&#xD;
      ),&#xD;
      local!ValuesArray:#&quot;7f34498f-2eb3-4308-a7f9-05a082324266&quot;(&#xD;
        local!FieldsValues&#xD;
      ), &#xD;
      local!CustomerId: index(&#xD;
        local!ValuesArray,&#xD;
        wherecontains(&#xD;
          &quot;CustomerId&quot;,&#xD;
          trim(&#xD;
            {&#xD;
              split(&#xD;
                tostring(&#xD;
                  local!FieldsArray&#xD;
                ),&#xD;
                &quot;;&quot;&#xD;
              )&#xD;
            }&#xD;
          )&#xD;
        ),&#xD;
        null&#xD;
      ),&#xD;
      local!CustomerId&#xD;
    ),&#xD;
    null&#xD;
  )&#xD;
)</definition>
        <namedTypedValue>
            <name>QueryCondition</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>IsMultiSelect</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
