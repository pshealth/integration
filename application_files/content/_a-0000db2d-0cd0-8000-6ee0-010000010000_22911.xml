<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>Utils_Field_UploadDocument</name>
        <uuid>_a-0000db2d-0cd0-8000-6ee0-010000010000_22911</uuid>
        <description></description>
        <parentUuid>_a-0000da7b-2d42-8000-6ee0-010000010000_1198</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>= load(
  local!doc,
  with(
    local!tempName: #&quot;SYSTEM_SYSRULES_fileUploadField&quot;(
      value: local!doc
    ).uploadedDocumentName,
    #&quot;SYSTEM_SYSRULES_fileUploadField&quot;(
      label: &quot;Upload New Version - Document&quot; &amp; ri!index,
      target: ri!documents[ri!index],
      value: local!doc,
      saveInto: {
        local!doc
      },
      validations: {
        if(
          isnull(
            ri!validation
          ),
          null,
          if(
            left(
              local!tempName,
              find(
                &quot;_&quot;,
                local!tempName
              ) - 1
            ) = ri!validation,
            &quot;&quot;,
            ri!validationMsg
          )
        )
      }
    )
  )
)</definition>
        <namedTypedValue>
            <name>documents</name>
            <type>
                <name>CollaborationDocument?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>index</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>label</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>folders</name>
            <type>
                <name>CollaborationFolder?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>validation</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>validationMsg</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role allowForAll="false" inherit="true" name="readers">
