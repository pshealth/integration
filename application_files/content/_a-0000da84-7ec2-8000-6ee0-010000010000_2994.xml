<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>SAIL_DynamicFileUpload_RenderField</name>
        <uuid>_a-0000da84-7ec2-8000-6ee0-010000010000_2994</uuid>
        <description></description>
        <parentUuid>_a-0000da84-7ec2-8000-6ee0-010000010000_2983</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=with(
  local!paddedArray: append(ri!files, null),
  #&quot;SYSTEM_SYSRULES_fileUploadField&quot;(
    label: if(ri!index = 1, ri!label, &quot;&quot;),
    target: ri!target,
    value: local!paddedArray[ri!index],
    saveInto: {
      a!save(ri!files, #&quot;_a-0000da84-7ec2-8000-6ee0-010000010000_2992&quot;(ri!files, ri!index, save!value))
    }
  )
)</definition>
        <namedTypedValue>
            <name>label</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>files</name>
            <type>
                <name>CollaborationDocument?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>target</name>
            <type>
                <name>CollaborationDocumentOrFolder</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>index</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
