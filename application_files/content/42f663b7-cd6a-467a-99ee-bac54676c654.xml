<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>CM_MI_GenerateDynamicLinkForSLAType</name>
        <uuid>42f663b7-cd6a-467a-99ee-bac54676c654</uuid>
        <description>Generates dynamic link for SLA Types</description>
        <parentUuid>_a-0000db0d-1b74-8000-6ee0-010000010000_10791</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
