<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>CM_MI_GetSLAMetIconCaptionValues</name>
        <uuid>e25cded9-533c-437d-8fed-f41d57bd142c</uuid>
        <description>Returns the caption values for sla met icons</description>
        <parentUuid>_a-0000db0d-1b74-8000-6ee0-010000010000_10791</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(ri!SLAMet=1,&quot;SLA Met&quot;,&#xD;
if(ri!SLAMet=0,if(not(#&quot;_a-0000d33c-c5a1-8000-7bd7-760000760000_1541&quot;(ri!EndDate)),&quot;DeadLine Missed&quot;,if(ri!DeadLine&gt;(now()-7),&quot;DeadLine Within 1 Week&quot;,&quot;DeadLine More Than 1 Week&quot;)),&quot;&quot;))</definition>
        <namedTypedValue>
            <name>SLAMet</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>DeadLine</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>EndDate</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
