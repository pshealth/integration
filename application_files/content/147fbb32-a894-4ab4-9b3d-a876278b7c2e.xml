    <rule>
        <name>SAIL_gridEditable</name>
        <uuid>147fbb32-a894-4ab4-9b3d-a876278b7c2e</uuid>
        <description></description>
        <parentUuid>_a-0000da7b-2d42-8000-6ee0-010000010000_1198</parentUuid>
        <visibility>
