            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=#&quot;SYSTEM_SYSRULES_gridField&quot;(&#xD;
			       label: ri!gridLabel,&#xD;
			      totalCount: ri!data.totalCount,&#xD;
			      columns: {&#xD;
			      		apply(#&quot;_a-0000da7b-2d42-8000-6ee0-010000010000_1199&quot;(_, _, ri!data.data),&#xD;
			      		ri!columnLabels,&#xD;
			      		ri!columnValues&#xD;
			      		)&#xD;
			      	},&#xD;
				 	 value: ri!gridSelection,&#xD;
				     saveInto: &#xD;
				     	{&#xD;
				     		ri!gridSelection,&#xD;
				     		ri!primaryKeys &lt;&lt; index(_, &quot;selected&quot;, null)&#xD;
				     	},&#xD;
				  	 selection: true,&#xD;
				     requireSelection: true&#xD;
					)</definition>
        <namedTypedValue>
            <name>data</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
