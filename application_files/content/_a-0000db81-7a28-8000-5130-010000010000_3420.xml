<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>Utils_CheckIsInputNumeric</name>
        <uuid>_a-0000db81-7a28-8000-5130-010000010000_3420</uuid>
        <description>Returns true if the input is numeric otherwise returns false</description>
        <parentUuid>_a-0000da6a-36ed-8000-6ee0-010000010000_946</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>= if(&#xD;
  or(&#xD;
    isnull(&#xD;
      ri!value&#xD;
    ),&#xD;
    ri!value = &quot;&quot;&#xD;
  ),&#xD;
  false(),&#xD;
  regexmatch(&#xD;
    &quot;^[0-9]*$&quot;,&#xD;
    ri!value&#xD;
  )&#xD;
)</definition>
        <namedTypedValue>
            <name>value</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
