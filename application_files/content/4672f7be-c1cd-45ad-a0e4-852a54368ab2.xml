<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>Utils_SearchValueByFindingIndex</name>
        <uuid>4672f7be-c1cd-45ad-a0e4-852a54368ab2</uuid>
        <description>Finds the index of Search Values from the Search Array and then returns the list of values find on that indexes from index array</description>
        <parentUuid>_a-0000da6a-36ed-8000-6ee0-010000010000_946</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    #&quot;_a-0000d33c-c5a1-8000-7bd7-760000760000_1541&quot;(
      ri!SearchValues
    ),
    #&quot;_a-0000d33c-c5a1-8000-7bd7-760000760000_1541&quot;(
      ri!SearchArray
    ),
    #&quot;_a-0000d33c-c5a1-8000-7bd7-760000760000_1541&quot;(
      ri!IndexArray
    )
  ),
  {},
  index(
    ri!IndexArray,
    apply(
      wherecontains(
        _,
        ri!SearchArray
      ),
      {ri!SearchValues}
    ),
    {}
  )
)</definition>
        <namedTypedValue>
            <name>SearchValues</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>SearchArray</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>IndexArray</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
